.myseeting-page {
  width: 750rpx;
  height: 100vh;
  position: relative;
}
.myseeting-page .myseeting-nav-warp {
  width: 100%;
  height: 50px;
  display: flex;
  justify-content: center;
}
.myseeting-page .myseeting-nav-warp .myseeting-nav {
  width: 90%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.myseeting-page .myseeting-nav-warp .myseeting-nav .back i {
  font-size: 26px;
  font-weight: 700;
}
.myseeting-page .myseeting-nav-warp .myseeting-nav .text {
  font-size: 14px;
  font-weight: 700;
}
.myseeting-page .seeting-header {
  width: 100%;
  height: 250px;
  background-color: red;
}
.myseeting-page .seeting-header .header-bg {
  width: 100%;
  height: 100%;
}
.myseeting-page .seeting-header .header-bg .seeting-background {
  width: 100%;
  height: 100%;
}
.myseeting-page .seeting-body-warp {
  width: 100%;
  height: 500px;
  background-color: #fff;
  position: absolute;
  top: 32%;
  border-radius: 30px 30px 0 0;
}
.myseeting-page .seeting-body-warp .seeting-body {
  width: 100%;
  height: 100%;
  position: relative;
  top: 0%;
  left: 0%;
  display: flex;
  justify-content: center;
}
.myseeting-page .seeting-body-warp .seeting-body .user-icon {
  width: 80px;
  height: 80px;
  border-radius: 50%;
  border: 5px solid #fff;
  position: absolute;
  top: 0%;
  left: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
}
.myseeting-page .seeting-body-warp .seeting-body .informations-items {
  width: 90%;
  height: 100%;
  padding-top: 50px;
}