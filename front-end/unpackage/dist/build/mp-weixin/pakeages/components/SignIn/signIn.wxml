<view class="all"><view class="bar"><view class="previous" bindtap="{{b}}"><button class="barbtn">{{a}}</button></view><view class="date">{{c}} 年 {{d}} 月</view><view class="next" bindtap="{{f}}"><button class="barbtn">{{e}}</button></view></view><view class="week-area"><view wx:for="{{g}}" wx:for-item="item" wx:key="b" class="week-txt">{{item.a}}</view></view><view class="myDateTable"><view wx:for="{{h}}" wx:for-item="item" wx:key="j" class="dateCell"><view wx:if="{{item.a}}" class="cell"></view><block wx:else><view wx:if="{{item.b}}" class="cell greenColor bgWhite">{{item.c}}</view><view wx:elif="{{item.d}}" bindtap="{{item.f}}" class="cell outSignStyle">{{item.e}} <view class="redDot"></view></view><view wx:elif="{{item.g}}" bindtap="{{item.h}}" class="cell whiteColor bgBlue"> 签到 </view><view wx:else class="whiteColor cell">{{item.i}}</view></block></view></view></view>